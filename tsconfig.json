{
    "compilerOptions": {
      "lib": ["webworker"],
      "module": "CommonJS",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "target": "ES2017",
      "alwaysStrict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "strictFunctionTypes": true,
      "strictNullChecks": true
    },
    "exclude": ["node_modules"],
    "include": ["**/*.ts"]
  }